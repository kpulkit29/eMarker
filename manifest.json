{
    "name": "eBookmarker",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["tabs", "storage","http://*/*","https://*/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "manifest_version": 2
  }